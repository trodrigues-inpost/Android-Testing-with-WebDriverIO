		"@types/sinon": "17.0.4",
		"@types/uuid": "10.0.0",
		"@typescript-eslint/eslint-plugin": "8.28.0",
		"@typescript-eslint/parser": "8.28.0",
		"@wdio/allure-reporter": "9.11.0",
		"@wdio/appium-service": "9.12.1",
		"@wdio/browserstack-service": "9.12.1",
		"@wdio/cli": "9.12.1",
		"@wdio/local-runner": "9.12.1",
		"@wdio/mocha-framework": "9.12.1",
		"@xmldom/xmldom": "0.9.8",
		"eslint": "9.23.0",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-wdio": "9.9.1",
		"jiti": "2.4.2",
		"prettier": "3.5.3",
		"sinon": "20.0.0",
		"ts-node": "10.9.2",
		"typescript": "5.8.2"
	},
	"dependencies": {
		"@faker-js/faker": "9.6.0",
		"allure-commandline": "2.33.0",
		"date-fns": "4.1.0",
		"dotenv": "16.4.7",
		"rimraf": "6.0.1",
		"semver": "7.7.1",
		"uuid": "11.1.0"
	},
	"scripts": {
		"tests:android:local": "npm run cleanup && wdio config/android.local.conf.ts",
		"cleanup": "rimraf allure-results && npx tsc && npm run fixcode",
		"checkcode": "eslint --fix --ext .ts .",
		"fixcode": "prettier --log-level=silent --write . && eslint --fix --ext .ts . --fix",
		"lint": "prettier --config .prettierrc . --write && eslint --fix --ext .ts . --fix",
		"kill": "adb kill-server && adb devices",
		"allureGen": "allure generate --clean",
		"allureOpen": "allure open",
		"allure": "npm run allureGen && npm run allureOpen",
		"androidEmulator": "emulator -avd Pixel_8_API_35"
	}
}
